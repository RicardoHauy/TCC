	<queryString>
		<![CDATA[SELECT pes.codpessoa, pes.nome, ac.codacao, parc.valorparcela, parc.dataapagar from pessoa pes, pessoaacao pa, acao ac, divida div, parcela parc where pes.codpessoa=pa.codpessoa and pa.codacao=ac.codacao and ac.codacao=div.codacao and div.coddivida=parc.coddivida and pa.tipopessoa=1 and dataapagar<sysdate and parc.dataquepagou is null]]>
